    "node-sass": "^4.13.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "~1.14.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-uglify": "^6.0.4",
