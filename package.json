{
  "name": "vue3-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "tolink": "npm link external/vue3 & npm link external/ui",
    "lint": "prettier --write --parser typescript \"packages/**/*.ts?(x)\" \"dev/**/*.ts?(x)\""
  },
  "dependencies": {},
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-typescript": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "prettier": "~1.14.0",
    "typescript": "^3.7.0",
    "vue-template-compiler": "^2.6.10",
    "at-ui-style": "^1.5.1"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "gitHooks": {
    "pre-commit": "npm run lint",
    "commit-msg": "node  script/verifyCommit.js"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}