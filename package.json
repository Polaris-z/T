{
  "name": "vue3-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "linkto": "npm link external/ui",
    "lint": "prettier --write --parser typescript \"packages/**/*.ts?(x)\" \"dev/**/*.ts?(x)\""
  },
  "dependencies": {
    "next-vue": "^3.0.0-alpha.10"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-typescript": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "at-ui-style": "^1.5.1",
    "highlight.js": "^9.16.2",
    "markdown-it": "^10.0.0",
    "markdown-it-anchor": "^5.2.5",
    "markdown-it-container": "^2.0.0",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-table-of-contents": "^0.4.4",
    "prettier": "~1.14.0",
    "lint-staged": "^9.5.0",
    "typescript": "^3.7.0",
    "vue-template-compiler": "^2.6.10"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "gitHooks": {
    "commit-msg": "node  script/verifyCommit.js"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ],
    "*.ts?(x)": [
      "prettier  --write",
      "git add"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
