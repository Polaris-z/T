{
  "name": "vue3-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cd example && yarn dev",
    "build": "npx rollup -c",
    "lint": "prettier --write  \"packages/**/*.ts?(x)\" \"dev/**/*.ts?(x)\""
  },
  "dependencies": {
    "vue": "3.0.0-alpha.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-typescript": "^7.7.7",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "autoprefixer": "^9.7.3",
    "next-vue": "^3.0.0-alpha.10",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-uglify": "^6.0.4",
    "highlight.js": "^9.16.2",
    "lint-staged": "^9.5.0",
    "markdown-it": "^10.0.0",
    "markdown-it-anchor": "^5.2.5",
    "markdown-it-container": "^2.0.0",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-table-of-contents": "^0.4.4",
    "node-sass": "^4.13.0",
    "prettier": "~1.14.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-vue": "^5.1.4",
    "sass-loader": "^8.0.0",
    "typescript": "^3.7.3"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node  script/verifyCommit.js"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ],
    "*.ts?(x)": [
      "prettier --parser typescript  --write",
      "git add"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}